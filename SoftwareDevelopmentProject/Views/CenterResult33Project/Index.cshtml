@model IEnumerable<SoftwareDevelopmentProject.Models.tbl_result33_project>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutCenter.cshtml";
}

<h2 class="text-center mt-3">3rd Semester DCSA 3304 Subject Mark</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-info btn-outline-dark" })
</p>
<table class="table table-striped table-hover table-responsive">
    <tr>
        <th>
            No
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_students.st_registration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_students.st_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_semester33_subject.s33_course_code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.project_report)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.viva)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.exam_term)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.st_study_center)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.submit_date)
        </th>


        <th>Action</th>
    </tr>
    @{ int i = 1; }
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @i
            </td>
            @{ i++; }
            <td>
                @Html.DisplayFor(modelItem => item.tbl_students.st_registration)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_students.st_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tbl_semester33_subject.s33_course_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.project_report)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.viva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.exam_term)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.st_study_center)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.submit_date)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.result33_project_id }, new { @class = "btn btn-outline-secondary" })
            </td>
        </tr>
    }

</table>
